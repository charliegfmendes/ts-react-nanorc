set softwrap
set tabsize 2
set tabstospaces
##set linenumbers
set autoindent

syntax "javascript" "typescript" "\.(js|jsx|ts|tsx)?$"

icolor blue "\<[a-zA-Z_][a-zA-Z0-9_]*\>"

icolor lightblue "\<(const|let|var|function|class|new|if|else|async|try|catch|throw|true|false|private|readonly)\>"

icolor magenta "\<(import|from|as|export|default|return|await)\>"

icolor yellow "[a-z0-9]+\("

color cyan "\<[A-Z][a-zA-Z0-9_]*\>"
icolor cyan "\<(string|number|boolean|null|undefined|symbol|void|any|never|object|bigint|array|tuple|unknown|void)\>"

icolor white "\)" "\(" "\$" "\{" "\}" "\." "<|>" "\@" "\=" "\/" "\:" "\," "\<[0-9]*\>" ":"

icolor green "//.*$"
icolor green start="/\*" end="\*/"

icolor orange start=""" end="""
icolor orange "'.*'"
icolor orange start="`" end="`"
